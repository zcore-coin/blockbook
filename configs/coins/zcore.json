{
  "coin": {
    "name": "ZCore",
    "shortcut": "ZCR",
    "label": "ZCore",
    "alias": "zcore"
  },
  "ports": {
    "backend_rpc": 8095,
    "backend_message_queue": 38395,
    "blockbook_internal": 9095,
    "blockbook_public": 9195
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-zcore",
    "package_revision": "satoshilabs-1",
    "system_user": "zcore",
    "version": "2.0.0",
    "binary_url": "https://github.com/zcore-coin/zcore-2.0/releases/download/v2.0.0/zcore-2.0.0-x86_64-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "7e73f042bf834bbfc29f2c57b8769631ba0370e44b9e66923818589f27d95698",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "bin/zcore-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/zcored -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "mempoolexpiry": 72
    }
  },
  "blockbook": {
    "package_name": "blockbook-zcore",
    "system_user": "blockbook-zcore",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "subversion": "/ZCore Core:2.0.0/",
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 78792518,
      "slip44": 119,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "Erick Costa",
    "package_maintainer_email": "erick@zcore.cash"
  }
}
